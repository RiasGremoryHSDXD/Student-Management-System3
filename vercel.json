{
    "builds": [
      {
        "src": "api/index.php",
        "use": "vercel-php@0.6.0"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }